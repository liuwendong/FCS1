: fmt\n      });\n    },\n    //从相册选\n    galleryVideo: function galleryVideo() {\n      var self = this;\n      plus.gallery.pick(function (event) {\n        console.log(\"选择视频成功\");\n\n        for (var i in event.files) {\n          console.log(event.files[i]);\n          self.videoList.push(event.files[i]);\n        }\n      }, function (error) {\n        console.log(\"选择视频失败\" + error.message);\n      }, {\n        filter: \"video\",\n        multiple: true,\n        system: true\n      });\n    },\n    showActionSheet: function showActionSheet() {\n      var self = this;\n      var bts = [{\n        title: \"摄像\"\n      }, {\n        title: \"从相册选择\"\n      }];\n      plus.nativeUI.actionSheet({\n        cancel: \"取消\",\n        buttons: bts\n      }, function (e) {\n        if (e.index == 1) {\n          self.getVideo();\n        } else if (e.index == 2) {\n          self.galleryVideo();\n        }\n      });\n    },\n    fileDel: function fileDel(index) {\n      var self = this;\n      self.videoList.splice(index, 1);\n    }\n  },\n  computed: {\n    showVideo: function showVideo() {\n      return this.videoList.length > 0;\n    }\n  }\n};",{"version":3,"sources":["AddVideo.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoCA,eAAA;AACA,QAAA,UADA;AAEA,MAFA,kBAEA;AACA,WAAA;AACA,iBAAA;AADA,KAAA;AAGA,GANA;AAOA,WAAA;AACA;AACA,YAFA,sBAEA;AACA,UAAA,OAAA,IAAA;AACA,UAAA,MAAA,KAAA,MAAA,CAAA,SAAA,EAAA;AACA,UAAA,MAAA,IAAA,yBAAA,CAAA,CAAA,CAAA;AACA,UAAA,MAAA,IAAA,qBAAA,CAAA,CAAA,CAAA;AACA,UAAA,iBAAA,CACA,UAAA,IAAA,EAAA;AACA,aAAA,EAAA,CAAA,yBAAA,CACA,IADA,EAEA,UAAA,KAAA,EAAA;AACA,kBAAA,GAAA,CAAA,MAAA,UAAA,KAAA,QAAA,GAAA,MAAA,IAAA;AACA,eAAA,SAAA,CAAA,IAAA,CAAA,MAAA,UAAA,EAAA;AACA,SALA,EAMA,UAAA,CAAA,EAAA;AACA,kBAAA,GAAA,CAAA,aAAA,EAAA,OAAA;AACA,eAAA,QAAA,CAAA,KAAA,CAAA,cAAA,EAAA,OAAA;AACA,SATA;AAWA,OAbA,EAcA,UAAA,KAAA,EAAA;AACA,gBAAA,GAAA,CAAA,SAAA,MAAA,OAAA;AACA,OAhBA,EAiBA;AAAA,oBAAA,GAAA;AAAA,gBAAA;AAAA,OAjBA;AAmBA,KA1BA;AA2BA;AACA,gBA5BA,0BA4BA;AACA,UA