AA,GAAA,CAAA,uBAAA,EAAA,IAAA,CAAA,KAAA,SAAA;AACA;AACA,KAxBA;AAyBA,YAzBA,sBAyBA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,KA3BA;AA4BA,aA5BA,qBA4BA,GA5BA,EA4BA;AACA,cAAA,GAAA,CAAA,GAAA;AACA;AA9BA;AAjBA,CAAA","sourcesContent":["<template>\n    <div class=\"content\">\n        <div class=\"logo\">\n        <img src=\"~imgUrl/FCSLogo.jpg\"/>\n        </div>\n        <div class=\"tip\">催收APP</div>\n        <div class=\"loginInput\">\n            <div class=\"name border-bottom\">\n                <img class=\"nameSpan\" src=\"~imgUrl/用户名@2x.png\" />\n                <input class=\"nameInput\" type=\"text\" placeholder=\"请输入用户名\" v-model=\"name\"/>\n            </div>\n            <div class=\"pwd border-bottom\">\n                <img class=\"pwdSpan\" src=\"~imgUrl/密码@2x.png\"/>\n                <input\n                        class=\"pwdInput\"\n                        type=\"text\"\n                        v-show=\"isOpen\"\n                        v-model=\"pwd\"\n                        placeholder=\"请输入密码\"\n                />\n                <input\n                        class=\"pwdInput-pwd\"\n                        type=\"password\"\n                        v-show=\"!isOpen\"\n                        v-model=\"pwd\"\n                        placeholder=\"请输入密码\"\n                />\n                <img id=\"eyeId\" class=\"eye\" @click=\"openCloseClick\" :src=\"eyeImgUrl\"/>\n            </div>\n        </div>\n        <button\n                class=\"loginBtn\"\n                @click=\"loginBtnClick\"\n        >登录</button>\n        <router-link to=\"/check\">\n            <button\n                    class=\"loginBtn\"\n                    type=\"button\"\n            >登录2</button>\n        </router-link>\n        <!--提示语-->\n        <hint\n                :title=\"hintTitle\"\n                @hideHint=\"hideHint\"\n                :isShow=\"showHint\"\n        ></hint>\n        <!--加载中-->\n        <loading :isShowLoading=\"showLoading\"></loading>\n    </div>\n</template>\n\n<script>\nimport eyeOpen from \"../../../../static/眼睛-开@2x.png\";\nimport eyeClose from \"../../../../static/眼睛-闭@2x.png\";\nimport axios from \"axios\";\nimport Hint from \"../../comment/Hint\";\nimport Loading from \"../../comment/Loading\";\nexport default {\n  name: \"LoginContent\",\n  data() {\n    return {\n      isOpen: false,\n      pwd: \"\",\n      name: \"\",\n      eyeImgUrl: eyeClose,\n      hintTitle: \"\",\n      showHint: false,\n      showLoading: false\n    };\n  },\n  components: {\n    Hint,\n    Loading\n  },\n  methods: {\n    openCloseClick() {\n      if (this.isOpen) {\n        this.eyeImgUrl = eyeClose;\n        this.isOpen = !this.isOpen;\n        this.pwd = document.getElementsByClassName(\"pwdInput\")[0].value;\n      } else {\n        this.eyeImgUrl = eyeOpen;\n        this.isOpen = !this.isOpen;\n        this.pwd = document.getElementsByClassName(\"pwdInput-pwd\")[0].value;\n      }\n    },\n    loginBtnClick() {\n      if (this.name.length == 0) {\n        this.hintTitle = \"请输入用户名\";\n        this.showHint = true;\n      } else if (this.pwd.length == 0) {\n        this.hintTitle = \"请输入密码\";\n        this.showHint = true;\n      } else {\n        localStorage.clear();\n        this.$router.push(\"/myCase\");\n        axios.get(\"/static/tsconfig.json\").then(this.loginSucc);\n      }\n    },\n    hideHint() {\n      this.showHint = false;\n    },\n    loginSucc(res) {\n      console.log(res);\n    }\n  }\n};\n</script>\n\n<style lang=\"stylus\" scoped>\n    @import \"~styles/varibles.styl\"\n    .content\n        width: 100%\n        height: 100%\n        .logo\n            margin-top: 2rem\n        .tip\n            margin-top: 0.2rem\n            font-size: 0.4rem\n            color: $font-color\n            font-family: \"PingFang SC\"\n        .loginInput\n            width: 80%\n            margin-top: 0.84rem\n            margin-left: 10%\n            line-height: 0.8rem\n            .name\n                display: flex\n                .nameSpan\n                    width: .6rem\n                    height: .6rem\n                    float: left\n                .nameInput\n                    flex: 1\n                    margin-left: 0.1rem\n            .pwd\n                display: flex\n                .pwdSpan\n                    width: .6rem\n                    height: .6rem\n                    margin-top: .1rem\n                    float: left\n                .pwdInput\n                    flex: 1\n                    margin-left: 0.1rem\n                .pwdInput-pwd\n                    flex: 1\n                    margin-left: 0.1rem\n                .eye\n                    width: .6rem\n                    height: .6rem\n                    float: right\n        .loginBtn\n            margin-top: 0.6rem\n            width: 80%\n            line-height: 0.8rem\n            height: 0.8rem\n            background: $commitBtnColor\n            border-radius: 0.04rem\n            color: #ffffff\n            font-size: 0.4rem\n</style>\n"],"sourceRoot":"src/pages/login/component"}]}