= false;\n    },\n    // 请求数据\n    requestData(idNo) {\n      this.showLoading = true;\n      this.timer = setInterval(() => {\n        this.showLoading = false;\n        clearInterval(this.timer);\n        if (idNo == \"123456\") {\n          //没请求到数据\n          this.hintTitle = \"该身份证无案件\";\n          this.showHint = true;\n        } else {\n          //请求到数据\n          this.ishaveCase = true;\n          this.$emit(\"getCases\");\n        }\n      }, 2000);\n    },\n    gotoDetail(index) {\n      console.log(\"点击了第\" + index + \"个\");\n      this.$router.push({name:'caseDetail',params:this.cases[index]})\n    }\n  },\n  watch: {\n    ishaveCases: function(newVal) {\n      if (newVal === true) {\n        this.$nextTick(() => {\n          let bscrollDom = this.$refs.cases;\n          this.scroll = new Bscroll(bscrollDom, {\n            click: true,\n            scrollY: true,\n            pullUpLoad: {\n              threshold: -30\n            },\n            pullDownRefresh: {\n              threshold: 30,\n              stop: 20\n            }\n          });\n          this.scroll.on(\"pullingUp\", () => {\n            if (!this.showLoading) {\n              console.log(\"上拉加载\");\n              this.pageIndex++;\n              this.requestData(localStorage.idNo);\n            }\n            this.scroll.finishPullUp();\n          });\n          this.scroll.on(\"pullingDown\", () => {\n            if (!this.showLoading) {\n              console.log(\"下拉刷新\");\n              this.pageIndex = 0;\n              this.requestData(localStorage.idNo);\n            }\n            this.scroll.finishPullDown();\n          });\n        });\n      }\n    }\n  }\n};\n</script>\n\n<style lang=\"stylus\" scoped>\n    @import \"~styles/varibles.styl\"\n    .cases\n        overflow: hidden\n        position: absolute\n        top: 1.9rem\n        left: 0\n        bottom: .15rem\n        padding-bottom: .15rem\n        right: 0\n        .oneCase\n            width: 90%\n           